{
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "tabWidth": 2,
        "useTabs": false,
        "semi": false,
        "singleQuote": true,
        "quoteProps": "as-needed",
        "trailingComma": "es5",
        "bracketSpacing": true,
        "arrowParens": "avoid",
        "printWidth": 80,
        "endOfLine": "lf",
        "bracketSameLine": false,
        "jsxSingleQuote": true
      }
    ]
  }
}
